<h1>Listing host_family_photos</h1>

<table>
  <tr>
    <th>Host family name</th>
    <th>Location</th>
    <th></th>
	<th></th>
    <th></th>
  </tr>

<% @host_family_photos.each do |host_family_photo| %>
  <tr>
    <td><%= host_family_photo.host_family_info.f_nm %> <%= host_family_photo.host_family_info.l_nm %></td>
	<td><%= host_family_photo.host_family_info.city %>, <%= host_family_photo.host_family_info.state %> <%= host_family_photo.host_family_info.zip %></td>
    <td><%= link_to 'Show', host_family_photo %></td>
    <td><%= link_to 'Edit', edit_host_family_photo_path(host_family_photo) %></td>
    <td><%= link_to 'Destroy', host_family_photo, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Host family photo', new_host_family_photo_path %>
